192.168.0.182 node_name=k8s-master02

[node]
192.168.0.183 node_name=k8s-node01
192.168.0.184 node_name=k8s-node02

[etcd]
192.168.0.111 etcd_name=etcd-1
192.168.0.112 etcd_name=etcd-2
192.168.0.113 etcd_name=etcd-3

[lb]
# 如果部署单Master，该项忽略
192.168.0.86 lb_name=lb-master
192.168.0.87 lb_name=lb-backup

[k8s:children]
master
node

[newnode]
192.168.0.185 node_name=k8s-node03